      <insertScript>Y</insertScript>
      <script># For the further steps only the .csv files are required. Therefore, other files in this directory are deleted. 
# Moreover, the summary directory that is downloaded in some cases is deleted.

rm -R  -f ${ADDRESSES_DOWNLOAD_DIR}/summary
find ${ADDRESSES_DOWNLOAD_DIR} -type f -not -name '*.csv'  -delete

cd ${ADDRESSES_DOWNLOAD_DIR} 

# Move all .csv files in the root directory of the country and rename directory to "country=country_name" to get a pre-partitionated directory

for country_dir in * ; do 
    for country_sub_dir in $country_dir/* ; do 
        for file in $country_sub_dir/* ; do
            if [ -f $file ]; then
		#sed -i 1d cars.csv    &lt;- do this to remove the first line
                mv "$file" "$country_dir"; 
            fi
        done
    done
    mv "$country_dir" "country=$country_dir";
done</script>
